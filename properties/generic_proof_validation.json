{
    "event": "IncompleteVerification(uint256,bytes32)",
    "chain_name": "ethereum",
    "properties": { 
        "input_data": "call(ethereum, eth_getTransactionByHash, [$transaction_hash]).get(result).get(input)",
        "receiver": "$input_data.slice(162, 202).as(address)",
        "proof": "$input_data.slice(10, 74).as(hex)"
    },
    "pattern": 
       [
        "require($map.get($receiver) == $proof, $map.remove($receiver))",
        "$receiver == $payer_address"
    ]
}