{
    "event": "Deposit(uint8,bytes32,uint64,address,bytes)",
    "chain_name": "ethereum",
    "properties": {
        "function_signature": "0xacc1b42e",
        "resource_id": "$event_data.slice(66, 130)",
        "calldata": "$function_signature.push($resource_id)",
        "handler_address_full": "ethereum.call_method($calldata, $qbridge_contract, $ethereum_block_number.as(hex)).result",
        "handler_address": "$handler_address_full.slice(26, 66).as(hex)",
        "handler_signature": "0x35d0d5dc",
        "token_calldata": "$handler_signature.push($resource_id)",
        "token_address_full": "ethereum.call_method($token_calldata, $handler_address, $ethereum_block_number.as(hex)).result",
        "token_address": "$token_address_full.slice(26, 66).as(hex)"
    },
    "pattern": [
        "$token_address != 0x0"
    ]
}